<% if notice %>
  <aside id='notice'> <%= notice %> </aside>
<% end %>

<h1>Your Pragmatic Catalog</h1>

<ul class="catalog">
<div> <%= @counter_display %></div>
<% cache @products do %>
    <% @products.each do |product| %>
        <% cache product do %>
            <%= render product %>
        <% end %>
    <% end %>
<% end %>    
</ul>